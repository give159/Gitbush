            <!-- 表11: よく使うパターン -->
            <div class="table-section">
                <h2>🎯 よく使うコマンドパターン</h2>
                <table id="table11">
                    <thead>
                        <tr>
                            <th style="width: 20%;">シチュエーション</th>
                            <th style="width: 50%;">コマンドシーケンス</th>
                            <th style="width: 30%;">説明</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td><strong>新規プロジェクト作成</strong></td>
                            <td>
                                <span class="code">mkdir myapp</span><br>
                                <span class="code">cd myapp</span><br>
                                <span class="code">git init</span><br>
                                <span class="code">touch README.md</span><br>
                                <span class="code">git add .</span><br>
                                <span class="code">git commit -m "Initial commit"</span>
                            </td>
                            <td>フォルダ作成からコミットまで</td>
                        </tr>
                        <tr>
                            <td><strong>日常のコミット</strong></td>
                            <td>
                                <span class="code">git status</span><br>
                                <span class="code">git add .</span><br>
                                <span class="code">git commit -m "message"</span><br>
                                <span class="code">git push</span>
                            </td>
                            <td>確認→追加→コミット→プッシュ</td>
                        </tr>
                        <tr>
                            <td><strong>ブランチで開発</strong></td>
                            <td>
                                <span class="code">git checkout -b feature/new</span><br>
                                <span class="code">(コーディング)</span><br>
                                <span class="code">git add .</span><br>
                                <span class="code">git commit -m "新機能"</span><br>
                                <span class="code">git push -u origin feature/new</span>
                            </td>
                            <td>ブランチ作成から初回プッシュ</td>
                        </tr>
                        <tr>
                            <td><strong>朝の作業開始</strong></td>
                            <td>
                                <span class="code">cd /c/projects/myapp</span><br>
                                <span class="code">git checkout main</span><br>
                                <span class="code">git pull</span><br>
                                <span class="code">git checkout -b feature/today</span>
                            </td>
                            <td>最新取得して作業開始</td>
                        </tr>
                        <tr>
                            <td><strong>コンフリクト解決</strong></td>
                            <td>
                                <span class="code">git pull</span><br>
                                <span class="code">(CONFLICTと表示)</span><br>
                                <span class="code">code ファイル名</span><br>
                                <span class="code">(衝突箇所を修正)</span><br>
                                <span class="code">git add .</span><br>
                                <span class="code">git commit -m "コンフリクト解決"</span>
                            </td>
                            <td>衝突発生から解決まで</td>
                        </tr>
                        <tr>
                            <td><strong>既存プロジェクトに参加</strong></td>
                            <td>
                                <span class="code">cd /c/projects</span><br>
                                <span class="code">git clone git@github.com:user/repo.git</span><br>
                                <span class="code">cd repo</span><br>
                                <span class="code">git checkout -b feature/mywork</span>
                            </td>
                            <td>クローンから作業開始</td>
                        </tr>
                    </tbody>
                </table>
            </div>
            
            <!-- 表12: エラー対処 -->
            <div class="table-section">
                <h2>🆘 よくあるエラーと対処法</h2>
                <table id="table12">
                    <thead>
                        <tr>
                            <th style="width: 30%;">エラーメッセージ</th>
                            <th style="width: 30%;">原因</th>
                            <th style="width: 40%;">解決コマンド</th>
                        </tr>
                    </thead>
                    <tbody>
                        <tr>
                            <td><span class="code">Permission denied (publickey)</span></td>
                            <td>SSH鍵の認証エラー</td>
                            <td>
                                <span class="code">ssh -T git@github.com</span><br>
                                <span class="code">ssh-keygen -t ed25519 -C "email"</span><br>
                                → GitHubに公開鍵を再登録
                            </td>
                        </tr>
                        <tr>
                            <td><span class="code">Your branch is behind</span></td>
                            <td>リモートが進んでいる</td>
                            <td>
                                <span class="code">git pull origin main</span>
                            </td>
                        </tr>
                        <tr>
                            <td><span class="code">CONFLICT (content)</span></td>
                            <td>マージコンフリクト</td>
                            <td>
                                ファイルを開いて修正<br>
                                <span class="code">git add .</span><br>
                                <span class="code">git commit -m "解決"</span>
                            </td>
                        </tr>
                        <tr>
                            <td><span class="code">Please commit your changes</span></td>
                            <td>未コミットの変更あり</td>
                            <td>
                                <span class="code">git stash</span><br>
                                <span class="code">git pull</span><br>
                                <span class="code">git stash pop</span>
                            </td>
                        </tr>
                        <tr>
                            <td><span class="code">not a git repository</span></td>
                            <td>Gitリポジトリではない</td>
                            <td>
                                <span class="code">git init</span><br>
                                または正しいフォルダに移動
                            </td>
                        </tr>
                        <tr>
                            <td><span class="code">pathspec did not match</span></td>
                            <td>ブランチが存在しない</td>
                            <td>
                                <span class="code">git branch -a</span><br>
                                <span class="code">git fetch</span>
                            </td>
                        </tr>
                    </tbody>
                </table>
            </div>
            
            <div class="note">
                <h3>💡 使い方のヒント</h3>
                <ul style="list-style: none; padding-left: 0;">
                    <li>✅ 各コマンドの「コピー」ボタンで簡単コピペ</li>
                    <li>✅ 「印刷/PDF保存」で紙やPDFに出力可能</li>
                    <li>✅ 「全表をコピー」でテキストエディタに貼り付け可能</li>
                    <li>✅ 「CSV出力」でExcelで開ける形式に変換</li>
                    <li>✅ 頻度: ⭐⭐⭐(よく使う) ⭐⭐(たまに) ⭐(たまに)</li>
                </ul>
            </div>
        </div>
    </div>
    
    <script>
        function copyText(text) {
            navigator.clipboard.writeText(text).then(() => {
                const btn = event.target;
                const originalText = btn.textContent;
                btn.textContent = '✓ コピー完了!';
                btn.style.background = '#28a745';
                setTimeout(() => {
                    btn.textContent = originalText;
                    btn.style.background = '#28a745';
                }, 2000);
            });
        }
        
        function copyAllTables() {
            const tables = document.querySelectorAll('table');
            let allText = '# Git/GitBash コマンド完全一覧表\n\n';
            
            tables.forEach((table, index) => {
                const section = table.closest('.table-section');
                const title = section.querySelector('h2').textContent;
                allText += `\n## ${title}\n\n`;
                
                const rows = table.querySelectorAll('tr');
                rows.forEach((row, rowIndex) => {
                    const cells = row.querySelectorAll('th, td');
                    const rowText = Array.from(cells).map(cell => {
                        return cell.textContent.replace(/\s+/g, ' ').trim();
                    }).join('\t');
                    allText += rowText + '\n';
                });
            });
            
            navigator.clipboard.writeText(allText).then(() => {
                alert('✓ 全ての表をコピーしました！\nテキストエディタに貼り付けてください。');
            });
        }
        
        function exportToCSV() {
            const tables = document.querySelectorAll('table');
            let csv = '\uFEFF'; // BOM for Excel UTF-8
            
            tables.forEach((table, index) => {
                const section = table.closest('.table-section');
                const title = section.querySelector('h2').textContent;
                csv += `\n"${title}"\n`;
                
                const rows = table.querySelectorAll('tr');
                rows.forEach(row => {
                    const cells = row.querySelectorAll('th, td');
                    const rowData = Array.from(cells).map(cell => {
                        let text = cell.textContent.replace(/\s+/g, ' ').trim();
                        // CSV用にダブルクォートをエスケープ
                        text = text.replace(/"/g, '""');
                        return `"${text}"`;
                    }).join(',');
                    csv += rowData + '\n';
                });
                csv += '\n';
            });
            
            // CSVファイルとしてダウンロード
            const blob = new Blob([csv], { type: 'text/csv;charset=utf-8;' });
            const link = document.createElement('a');
            const url = URL.createObjectURL(blob);
            link.setAttribute('href', url);
            link.setAttribute('download', 'git-commands-complete-list.csv');
            link.style.visibility = 'hidden';
            document.body.appendChild(link);
            link.click();
            document.body.removeChild(link);
        }
    </script>
</body>
</html>
