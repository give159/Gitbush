    <script>
        function copyText(text) {
            navigator.clipboard.writeText(text).then(() => {
                const btn = event.target;
                const originalText = btn.textContent;
                btn.textContent = 'âœ“ ã‚³ãƒ”ãƒ¼å®Œäº†!';
                btn.style.background = '#28a745';
                setTimeout(() => {
                    btn.textContent = originalText;
                    btn.style.background = '#28a745';
                }, 2000);
            });
        }
        
        function copyAllTables() {
            const tables = document.querySelectorAll('table');
            let allText = '# Git/GitBash ã‚³ãƒãƒ³ãƒ‰å®Œå…¨ä¸€è¦§è¡¨\n\n';
            
            tables.forEach((table, index) => {
                const section = table.closest('.table-section');
                const title = section.querySelector('h2').textContent;
                allText += `\n## ${title}\n\n`;
                
                const rows = table.querySelectorAll('tr');
                rows.forEach((row, rowIndex) => {
                    const cells = row.querySelectorAll('th, td');
                    const rowText = Array.from(cells).map(cell => {
                        return cell.textContent.replace(/\s+/g, ' ').trim();
                    }).join('\t');
                    allText += rowText + '\n';
                });
            });
            
            navigator.clipboard.writeText(allText).then(() => {
                alert('âœ“ å…¨ã¦ã®è¡¨ã‚’ã‚³ãƒ”ãƒ¼ã—ã¾ã—ãŸï¼\nãƒ†ã‚­ã‚¹ãƒˆã‚¨ãƒ‡ã‚£ã‚¿ã«è²¼ã‚Šä»˜ã‘ã¦ãã ã•ã„ã€‚');
            });
        }
        
        function exportToCSV() {
            const tables = document.querySelectorAll('table');
            let csv = '\uFEFF'; // BOM for Excel UTF-8
            
            tables.forEach((table, index) => {
                const section = table.closest('.table-section');
                const title = section.querySelector('h2').textContent;
                csv += `\n"${title}"\n`;
                
                const rows = table.querySelectorAll('tr');
                rows.forEach(row => {
                    const cells = row.querySelectorAll('th, td');
                    const rowData = Array.from(cells).map(cell => {
                        let text = cell.textContent.replace(/\s+/g, ' ').trim();
                        // CSVç”¨ã«ãƒ€ãƒ–ãƒ«ã‚¯ã‚©ãƒ¼ãƒˆã‚’ã‚¨ã‚¹ã‚±ãƒ¼ãƒ—
                        text = text.replace(/"/g, '""');
                        return `"${text}"`;
                    }).join(',');
                    csv += rowData + '\n';
                });
                csv += '\n';
            });
            
            // CSVãƒ•ã‚¡ã‚¤ãƒ«ã¨ã—ã¦ãƒ€ã‚¦ãƒ³ãƒ­ãƒ¼ãƒ‰
            const blob = new Blob([csv], { type: 'text/csv;charset=utf-8;' });
            const link = document.createElement('a');
            const url = URL.createObjectURL(blob);
            link.setAttribute('href', url);
            link.setAttribute('download', 'git-commands-complete-list.csv');
            link.style.visibility = 'hidden';
            document.body.appendChild(link);
            link.click();
            document.body.removeChild(link);
        }
    </script>
</body>
