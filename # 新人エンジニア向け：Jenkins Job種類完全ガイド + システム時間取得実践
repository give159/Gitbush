        stage('æ™‚é–“æƒ…å ±è¡¨ç¤º') {
            steps {
                sh '''
                    echo "â° ã‚·ã‚¹ãƒ†ãƒ æ™‚é–“æƒ…å ±"
                    echo "----------------------------------------"
                    echo "  ç¾åœ¨æ—¥æ™‚: $(date '+%Yå¹´%mæœˆ%dæ—¥ %Hæ™‚%Måˆ†%Sç§’')"
                    echo "  ISOå½¢å¼: $(date '+%Y-%m-%d %H:%M:%S')"
                    echo "  UNIXã‚¿ã‚¤ãƒ : $(date +%s)"
