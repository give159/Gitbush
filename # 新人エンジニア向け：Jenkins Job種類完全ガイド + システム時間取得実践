    stage('ãƒ“ãƒ«ãƒ‰') {
        echo 'ãƒ“ãƒ«ãƒ‰ä¸­...'
        sh 'mvn clean install'
    }
    
    stage('ãƒ†ã‚¹ãƒˆ') {
        echo 'ãƒ†ã‚¹ãƒˆå®Ÿè¡Œä¸­...'
        sh 'mvn test'
    }
    
    stage('ãƒ‡ãƒ—ãƒ­ã‚¤') {
        echo 'ãƒ‡ãƒ—ãƒ­ã‚¤ä¸­...'
        sh './deploy.sh'
    }
}
```

